#####################################################################################################################
# /etc/openldap/data/06_groups.ldif
#####################################################################################################################
# Description:
#  This LDIF defines two working example groups.
#####################################################################################################################

#####################################################################################################################
# uid=4000014,ou=groups,dc=foss-cloud,dc=org
################################################################################
# Description:
#   This is an example group mapping with the name "Technology".
################################################################################
dn: uid=4000014,ou=groups,dc=foss-cloud,dc=org
objectclass: top
objectclass: sstGroupObjectClass
objectclass: sstRelationship
objectclass: labeledURIObject
uid: 4000014
sstDisplayName: Technology
description: This is an example group mapping with the name "Technology".
labeledURI: ldap:///ou=people,dc=foss-cloud,dc=org??one?(sstGroupUID=4000014)
sstBelongsToResellerUID: 4000000
sstBelongsToCustomerUID: 4000001

################################################################################
# uid=4000015,ou=groups,dc=foss-cloud,dc=org
################################################################################
# Description:
#   This is an example group mapping with the name "Office".
################################################################################
dn: uid=4000015,ou=groups,dc=foss-cloud,dc=org
objectclass: top
objectclass: sstGroupObjectClass
